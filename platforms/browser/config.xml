<?xml version='1.0' encoding='utf-8'?>
<widget id="com.wineshop.app" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>WINESHOP</name>
    <description>
        Wineshop Inventory Management
    </description>
    <author email="sales@webzin.in" href="https://www.webzin.in">
        Webzin Team
    </author>
    <content src="index.html" />
    <splash src="splash.png" />
    <preference name="SplashScreenDelay" value="5000" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
